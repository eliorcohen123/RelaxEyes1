<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.elior.relaxeyes">

    <application
        android:allowBackup="true"
        android:icon="@drawable/eye_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/eye_icon"
        android:supportsRtl="false"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.elior.relaxeyes.WelcomeActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DetailsOnClient"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.elior.relaxeyes.TimerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".WelcomeActivityRegulations"
            android:screenOrientation="portrait" />

        <receiver
            android:name=".MyReceiverAlarm"
            android:enabled="true"
            android:exported="true"
            android:process=":remote" />

        <service android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>